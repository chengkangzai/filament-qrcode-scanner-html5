{
    "name": "chengkangzai/filament-qrcode-scanner-html5",
    "description": "A Filament form action for scanning barcodes and QR codes using the device camera with html5-qrcode library",
    "keywords": [
        "laravel",
        "filament",
        "barcode",
        "qrcode",
        "scanner",
        "camera",
        "html5-qrcode"
    ],
    "homepage": "https://github.com/chengkangzai/filament-qrcode-scanner-html5",
    "license": "MIT",
    "authors": [
        {
            "name": "CCK",
            "email": "pycck@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0|^5.0",
        "livewire/livewire": "^3.0|^4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0|^11.0",
        "pestphp/pest": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "CCK\\FilamentQrcodeScannerHtml5\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCK\\FilamentQrcodeScannerHtml5\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CCK\\FilamentQrcodeScannerHtml5\\BarcodeScannerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
